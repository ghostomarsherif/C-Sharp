
//Write a C# program which prints out the values 1 to 10 on separate lines (Use WHILE statement)





using System;



namespace omar
{
    class Program
    {
        public static void Main(string[] args)
        {
            int i=1;
            while (i <= 10)
            {
                Console.WriteLine(i);
                i++;
            }


        }
    }
}


